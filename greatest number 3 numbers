# to check greatest between thrree numbers
x=int(input("Enter 1st number"))
y=int(input("Enter 2nd number"))
z=int(input("Enter 3rd number"))
if x>y:
    if z>x:
        print(f"{z} is greatest")
    else:
        print(f"{x} is greatest")
else:
    if z>y:
        print(f"{z} is greatest")
    else:
        print(f"{y} is greatest")
